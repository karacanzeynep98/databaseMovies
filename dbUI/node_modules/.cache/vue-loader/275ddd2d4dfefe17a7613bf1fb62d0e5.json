-group>\n\n      <b-form-group id=\"input-group-2\" label=\"Ranking:\" label-for=\"input-2\" :invalid-feedback=\"invalidFeedback\">\n        <b-form-input id=\"input-2\" v-model=\"form.actoractress\" placeholder=\"Enter actor/actress name\" :state=\"state\"> </b-form-input>\n      </b-form-group>\n\n      <b-form-group id=\"input-group-2\" label=\"Actor/Actress:\" label-for=\"input-2\" :invalid-feedback=\"invalidFeedback\">\n        <b-form-input id=\"input-2\" v-model=\"form.ranking\" placeholder=\"Enter ranking\" :state=\"state\"> </b-form-input>\n      </b-form-group>\n\n      <b-form-group id=\"input-group-3\" label=\"Genre:\" label-for=\"input-3\" :invalid-feedback=\"invalidFeedback\">\n        <b-form-input id=\"input-3\" v-model=\"form.genre\" placeholder=\"Enter genre\" :state=\"state\"> </b-form-input>\n      </b-form-group>\n\n      <b-form-group id=\"input-group-4\" label=\"Title:\" label-for=\"input-4\" :invalid-feedback=\"invalidFeedback\">\n        <b-form-input id=\"input-4\" v-model=\"form.title\" placeholder=\"Enter title\" :state=\"state\"> </b-form-input>\n      </b-form-group>\n\n      <b-button type=\"submit\" variant=\"primary\">Submit</b-button>\n      <b-button type=\"reset\" variant=\"danger\">Reset</b-button>\n    </b-form>\n    <!-- <listCenturies v-if=\"isPopulated\" v-bind:searchForm=\"form\"/> -->\n    \n  </div>\n</template>\n\n<script>\nexport default {\n    computed: {\n        invalidFeedback() {\n        if (this.form.director.length >= 4 || this.form.actoractress.length >= 4 || this.form.ranking.length >= 4 || this.form.genre.length >= 4|| this.form.title.length >= 4) {\n          return ''\n        } else {\n          return 'Please enter something'\n        }\n      }\n    },\n    data() {\n      return {\n        form: {\n          director: '',\n          actoractress: '',\n          ranking: '',\n          genre: '',\n          title: ''\n        },\n        show: true,\n        isPopulated: false, \n      }\n    },\n    methods: {\n      onSubmit(evt) {\n        evt.preventDefault()\n        //GET THE FORM OBJECT AND DO WHATEVER HERE!!!\n        // alert(JSON.stringify(this.form))\n        this.isPopulated = true;\n      },\n      onReset(evt) {\n        evt.preventDefault()\n        // Reset our form values\n        this.form.id = ''\n        this.form.name = ''\n        this.form.objectcount = ''\n        this.form.temporalorder = ''\n        // Trick to reset/clear native browser form validation state\n        this.show = false\n        this.$nextTick(() => {\n          this.show = true\n          this.isPopulated = false\n        })\n      },\n    },\n}\n\n\n\n\n\n</script>\n\n"]}]}